{
    "name": "@aoda-zhang/honey-service",
    "main": "dist/main.js",
    "version": "1.0.0",
    "scripts": {
        "prepare": "husky install",
        "build:PROD": "cross-env NODE_ENV=prod nest build",
        "build:UAT": "cross-env NODE_ENV=uat nest build",
        "format": "prettier --write",
        "dev": "cross-env NODE_ENV=dev nest start --watch",
        "uat": "cross-env NODE_ENV=uat nest start --watch",
        "test": "cross-env NODE_ENV=test nest start --watch",
        "start:UAT": "cross-env NODE_ENV=uat node dist/main.js",
        "start:PROD": "cross-env NODE_ENV=prod node dist/main.js",
        "start:debug": "nest start --debug --watch",
        "lint": "eslint --fix",
        "lint-staged": "lint-staged"
    },
    "lint-staged": {
        "src/**/*.{ts,js}": [
            "yarn run format",
            "yarn run lint"
        ]
    },
    "dependencies": {
        "@nestjs/common": "^9.0.0",
        "@nestjs/config": "^2.3.1",
        "@nestjs/core": "^9.0.5",
